{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "@viberlab/chips-autocomplete": [
        "dist/vbr-chips-autocomplete"
      ],
      "@viberlab/chips-autocomplete/*": [
        "dist/vbr-chips-autocomplete/*"
      ],
      "@viberlab/storage": [
        "dist/vbr-storage"
      ],
      "@viberlab/storage/*": [
        "dist/vbr-storage/*"
      ],
      "@viberlab/input-keyboard": [
        "dist/vbr-input-keyboard"
      ],
      "@viberlab/input-keyboard/*": [
        "dist/vbr-input-keyboard/*"
      ],
      "@viberlab/translate": [
        "dist/vbr-translate"
      ],
      "@viberlab/translate/*": [
        "dist/vbr-translate/*"
      ],
      "@viberlab/translate-loader": [
        "dist/vbr-translate-loader"
      ],
      "@viberlab/translate-loader/*": [
        "dist/vbr-translate-loader/*"
      ],
      "@viberlab/translate-pipe": [
        "dist/vbr-translate-pipe"
      ],
      "@viberlab/translate-pipe/*": [
        "dist/vbr-translate-pipe/*"
      ]
    }
  }
}
