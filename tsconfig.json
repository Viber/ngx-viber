{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "vbr-chips-autocomplete": [
        "dist/vbr-chips-autocomplete"
      ],
      "vbr-chips-autocomplete/*": [
        "dist/vbr-chips-autocomplete/*"
      ],
      "vbr-storage": [
        "dist/vbr-storage"
      ],
      "vbr-storage/*": [
        "dist/vbr-storage/*"
      ],
      "vbr-input-keyboard": [
        "dist/vbr-input-keyboard"
      ],
      "vbr-input-keyboard/*": [
        "dist/vbr-input-keyboard/*"
      ],
      "vbr-input-params-templates": [
        "dist/vbr-input-params-templates"
      ],
      "vbr-input-params-templates/*": [
        "dist/vbr-input-params-templates/*"
      ],
      "vbr-translate": [
        "dist/vbr-translate"
      ],
      "vbr-translate/*": [
        "dist/vbr-translate/*"
      ],
      "vbr-translate-loader": [
        "dist/vbr-translate-loader"
      ],
      "vbr-translate-loader/*": [
        "dist/vbr-translate-loader/*"
      ],
      "vbr-translate-pipe": [
        "dist/vbr-translate-pipe"
      ],
      "vbr-translate-pipe/*": [
        "dist/vbr-translate-pipe/*"
      ]
    }
  }
}